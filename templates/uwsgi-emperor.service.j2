[Unit]
Description=web2py uWSGI instance
After=network.target

[Service]
ExecStart=/usr/local/bin/uwsgi --ini /etc/uwsgi/web2py.ini
User=www-data
Group=www-data
RuntimeDirectory = uwsgi
Restart = always
KillSignal = SIGQUIT
Type = notify
StandardError = syslog
NotifyAccess = all

[Install]
WantedBy=multi-user.target
